	<parameter name="idClasificacion" class="java.lang.String">
		<parameterDescription><![CDATA[id de la clasificacion]]></parameterDescription>
	</parameter>
	<queryString>
		<![CDATA[SELECT bodClas.noparte,
              bodClas.descripcion,
              bodClas.descripcion_ing,
              bodCli.Nom AS cliente,
              procli.proNom AS proveedor,
              bodClas.fundamento_legal,
              bodClas.fraccion,
              GROUP_CONCAT(bodClasCat.descripcion SEPARATOR ', ') AS RYRNAS,
              IF(bodClasDocs.nombre IS NULL, '', CONCAT('https://www.delbravoweb.com/archivos/monitor/clasificaBodega/', bodClas.id, '/', bodClasDocs.nombre)) AS imagen
FROM bodega.clasificaciones AS bodClas INNER JOIN
            bodega.clientes AS bodCli ON bodCli.Cliente_id=bodClas.cliente_id INNER JOIN
            bodega.procli ON procli.proveedor_id=bodClas.proveedor_id LEFT JOIN 
            bodega.clasificaciones_restricciones AS bodClasRest ON bodClasRest.id_clasificacion=bodClas.id INNER JOIN
            bodega.clasificaciones_catalogo AS bodClasCat ON bodClasCat.id=bodClasRest.id_restriccion LEFT JOIN
            bodega.clasificaciones_docs AS bodClasDocs ON bodClasDocs.id_clasificacion=bodClas.id AND
                                                                                                    bodClasDocs.principal=1
WHERE bodClas.id = $P{idClasificacion}
GROUP BY bodClas.id]]>
	</queryString>
